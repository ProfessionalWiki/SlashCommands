{
  "name": "visual-editor",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "type-check": "tsc --noEmit -p tsconfig.vitest.json --composite false",
    "build-only": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint-no-fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.16",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.2.3",
    "@types/oojs": "^6.0.4",
    "@types/oojs-ui": "^0.46.1",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@wmde/eslint-config-wikimedia-typescript": "^0.2.5",
    "eslint": "^8.41.0",
    "eslint-config-wikimedia": "^0.24.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "oojs-ui": "^0.47.0",
    "sass": "^1.62.1",
    "stylelint-config-wikimedia": "^0.15.0",
    "types-mediawiki": "^1.3.0",
    "typescript": "^5.0.4",
    "vite": "^4.0.4"
  }
}
